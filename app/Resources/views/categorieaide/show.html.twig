{% extends 'dash.html.twig' %}

{% block content %}
    <h1>Categorie {{ categorieAide.nom }}</h1>

    <table class="table table-striped jambo_table bulk_action">
        <tbody>

            <tr>
                <th>Nom</th>
                <td>{{ categorieAide.nom }}</td>
            </tr>
            <tr>
                <th>Icone</th>
                <td>{{ categorieAide.icone }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('categorieaide_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('categorieaide_edit', { 'id': categorieAide.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
